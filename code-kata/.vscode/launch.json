{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Docker Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "SearchFunctionality.Tests/bin/Debug/net8.0/SearchFunctionality.Tests.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "justMyCode": true,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/workspace": "${workspaceFolder}"
            }
        }
    ],
    
}
